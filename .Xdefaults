!             __    ___          ____
!    __ _____/ /__ / _/__ ___ __/ / /____
!  _ \ \ / _  / -_) _/ _ `/ // / / __(_-<
! (_)_\_\\_,_/\__/_/ \_,_/\_,_/_/\__/___/
!

! fonts
*font: -uw-ttyp0-medium-r-normal--11-100-75-75-c-60-iso10646-1
*boldFont: -uw-ttyp0-medium-r-normal--11-100-75-75-c-60-iso10646-1


! disable urxvts ugly scrollbar
*scrollBar: false


! colorscheme
! special
*.foreground:   #b9b9b9
*.background:   #101010
*.cursorColor:  #b9b9b9

! black
*.color0:       #101010
*.color8:       #525252

! red
*.color1:       #f2585b
*.color9:       #c75c5e

! green
*.color2:       #8e8e8e
*.color10:      #8e8e8e

! yellow
*.color3:       #a0a0a0
*.color11:      #a0a0a0

! blue
*.color4:       #686868
*.color12:      #686868

! magenta
*.color5:       #747474
*.color13:      #747474

! cyan
*.color6:       #868686
*.color14:      #868686

! white
*.color7:       #b9b9b9
*.color15:      #f7f7f7

! cursor
*.cursorColor:  #eeeeee
*cursorBlink: 1


! size
*geometry: 84x22


! padding
*internalBorder: 10


! xft fonts settings
Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault


! rofi settings
rofi.color-enabled: true

! Property            Background      Foreground      Background Alt       Highlight BG      Highlight FG
rofi.color-window:    #050505,        #FFFFFF,        #050505,             #c9c9c9,          #050505
rofi.color-normal:    #050505,        #FFFFFF,        #050505,             #c9c9c9,          #050505
rofi.color-active:    #050505,        #FFFFFF,        #050505,             #c9c9c9,          #050505
rofi.color-urgent:    #050505,        #DDDDDD,        #050505,             #c9c9c9,          #050505
rofi.font: Roboto 12
